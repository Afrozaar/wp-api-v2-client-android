<resources>
    <string name="app_name">wp-api-v2-client-android</string>

    <!-- WP defaults -->
    <string name="user_role">author</string>

    <string name="post_type">gallery</string>
    <string name="content_image_uri"><![CDATA[<a href="%1$s"><img src="%1$s" alt="%2$s" /></a>]]></string>
    <string name="content_video_uri">
        <![CDATA[<div id="thePlayer%2$s"></div><script type="text/javascript">jwplayer(\"thePlayer%2$s\").setup({
            flashplayer: \"http://static.videoninja101.com/jwplayer/player.swf\", file: \"%1$s\"});
            jwplayer(\"thePlayer%2$s\").addButton(
    \"https://www.afrozaar.com/wp-content/uploads/2016/04/download.png\",
    \"Download Video\",
    function() {
      window.location.href = jwplayer(\"thePlayer%2$s\").getPlaylist()[0].sources[0].file;
    },
    \"download\"
);
            </script>
        ]]>
    </string>

    <string name="content_video_uri_transcode">
        <![CDATA[
        <div class=\"player-wrapper%2$s\">
        <div id=\"player%2$s\">
        </div>
        <div id="downloads">
            <a class=\"raw-video-download\" href=\"http://cdn.mojoreporter.com/%1$s.mp4\" download><img src="http://cdn.mojoreporter.com/conf/downloadVideo.png" alt="Download the raw video"></a>
            <a href=\"http://cdn.mojoreporter.com/%1$s.mp3\" download><img src="http://cdn.mojoreporter.com/conf/downloadAudio.png" alt="Download the audio"></a>
        </div>

        <script type="text/javascript">
            var player = bitdash(\"player%2$s\");

            var conf = {
                key: \'00fd7ae2-a611-40b3-8fdf-813166e6b78a\',
                source: {
                    dash: \'http://cdn.mojoreporter.com/%1$s.mpd\',
                    hls: \'http://cdn.mojoreporter.com/%1$s.m3u8\',
                    progressive: \'http://cdn.mojoreporter.com/%1$s.mp4\'
                },
                style: {
                    aspectratio: \'16:9\',
                    controls: true
                },
                skin: \'http://cdn.mojoreporter.com/conf/mojoskin.json\'
            };

            player.setup(conf).then(function (value) {
                console.log(\"Successfully created bitdash player instance\");
            }, function (reason) {
                console.log(\"Error while creating bitdash player instance\");
            });
        </script>
        </div>]]>
    </string>

    <string name="content_video_shortcode">
        [baobab_video url=http://cdn.mojoreporter.com/%1$s.mp4]
    </string>

